{
  "name": "@json-schema-language/json-schema-language",
  "version": "0.1.3",
  "description": "A TypeScript / JavaScript / Node.js implementation of JSON Schema Language",
  "main": "lib/index.js",
  "repository": "ssh://git@github.com/json-schema-language/json-schema-language-typescript.git",
  "author": "Ulysse Carion <ulyssecarion@gmail.com>",
  "license": "MIT",
  "files": [
    "lib"
  ],
  "dependencies": {
    "@json-schema-language/json-pointer": "^0.1.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.8",
    "jest": "^24.7.1",
    "prettier": "^1.17.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "lint": "tslint --project .",
    "fmt": "prettier --write src/**/*.ts"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "rootDir": "src",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "js",
      "ts"
    ]
